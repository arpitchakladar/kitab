<UserControl xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:vm="using:KiTab.ViewModels"
	x:DataType="vm:PDFViewerViewModel"
	x:Class="KiTab.Views.PDFViewerView"
>
	<StackPanel Margin="4 0 0 4" Grid.Column="0" Background="#555555">
		<StackPanel VerticalAlignment="Top" HorizontalAlignment="Left" Width="20" Margin="4 -2 4 4">
			<Image Source="{SvgImage Assets/Images/bookmark.svg}" />
		</StackPanel>
		<Image Source="{Binding Page}"/>
	</StackPanel>
</UserControl>
